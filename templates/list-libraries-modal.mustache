<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
    <h4 id="modal-title" class="modal-title">List libraries from registry</h4>
</div>
<div id="modal-body" class="modal-body">
    <p id="modal-error" class="alert alert-danger hide"></p>
    <div class="form-horizontal">
        <div class="form-group">
            <label for="libs-package" class="col-sm-2 control-label">package</label>
            <div class="col-sm-10">
                <input type="text" id="libs-package" class="form-control input-sm" placeholder="org.kevoree.library.js">
                <p class="help-block">e.g <strong>org.kevoree.library.js</strong></p>
            </div>
        </div>
    </div>
    <div id="libs-list"></div>
</div>
<div class="modal-footer">
    <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
    <button id="merge-libs-btn" type="button" class="btn btn-primary">Merge libraries</button>
</div>
